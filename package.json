{
  "name": "podcast-app",
  "version": "1.0.0",
  "description": "A simple app to listen to podcast",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "build": "npm run concat-css && npm run concat-grid-css && webpack -p && cp playcast-ui/src/index.html playcast-ui/build/index.html",
    "devEnv": "npm run build && npm start",
    "concat-css": "cat playcast-ui/src/app/global_styles/* > playcast-ui/build/bundle.css",
    "concat-grid-css": "cat playcast-ui/src/app/grid_system/src/* > playcast-ui/src/app/grid_system/grid.css"
  },
  "author": "Renny A. Torres",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "fast-xml-parser": "^3.12.5",
    "he": "^1.2.0",
    "request": "^2.88.0",
    "axios": "^0.18.0",
    "babel-plugin-react-css-modules": "^5.0.0",
    "base64-url": "^2.2.0",
    "query-string": "^6.2.0",
    "react": "^16.5.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "react-share": "^2.4.0",
    "strict-uri-encode": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-loader": "^7.1.5",
    "css-loader": "^2.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.1.2",
    "webpack-combine-loaders": "^2.0.4"
  }
}
